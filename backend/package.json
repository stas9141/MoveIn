{
  "name": "movein-backend",
  "version": "1.0.0",
  "description": "Backend API for MoveIn app with JWT authentication and token refresh",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "start:simple": "node src/app-simple.js",
    "dev": "nodemon src/app.js",
    "dev:simple": "nodemon src/app-simple.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:seed": "node prisma/seed.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/controllers/ tests/auth.test.js",
    "test:integration": "jest tests/integration/",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "keywords": [
    "nodejs",
    "express",
    "jwt",
    "authentication",
    "postgresql",
    "prisma"
  ],
  "author": "MoveIn Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "nodemailer": "^6.9.7",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.4.1",
    "jwks-rsa": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
